import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        a = Math.abs(a);
        int last = a % 10;
        if(a == 0) {
            System.out.println("0");
            return;
        }
        while(a >= 10)
        {
            a = a/10;
        }
        // int first = a % 10;
        int res = a + last;
        System.out.print(res);
    }
}
