import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s  = new Scanner(System.in);
        int a = s.nextInt();
        int b  = a;
        int sum =0;
        int x;
        while(a > 0 )
        {
            x = a % 10;
            sum= sum +x;
            a =a /10;
        }
        int  y = b % sum;
        if(y == 0) System.out.println("Harshad Number");
        else System.out.println("Not Harshad Number");
        
    }
}
