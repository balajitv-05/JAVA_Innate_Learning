import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner (System.in);
        int a = s.nextInt();
        boolean b =neon(a);
        System.out.println(b);
        
    }
    public static boolean neon (int a)
    {
        int b = a * a;
        int sum = 0;
        while (b != 0)
        {
            int x = b % 10;
            sum = sum + x;
            b = b/10;
        }
        if(sum == a) return true;
        else return false;
        
    }
}
