import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s =new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        int c = pow(a,b);
        System.out.println(c);
    }
    public static int pow(int a,int b)
    {
        int x=1;
        if(b == 0) return 1;
        for(int i = 1;i<=b;i++)
        {
            x = x *a; 
        }
        return x;
    }
}
