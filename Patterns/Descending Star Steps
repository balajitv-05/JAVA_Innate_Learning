import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int row = s.nextInt();
        for(int i = row;i>0;i--)
        {
            for(int j = 0; j<i;j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
